{
  "extends": [
    "wesbos",
    // "wesbos/typescript",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-console": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2,
        "arrowParens": "avoid"
      }
    ]
  }
}
